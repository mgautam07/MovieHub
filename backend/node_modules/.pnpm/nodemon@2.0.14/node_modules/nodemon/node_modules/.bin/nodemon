#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules/.pnpm/nodemon@2.0.14/node_modules/nodemon/bin/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules/.pnpm/nodemon@2.0.14/node_modules/nodemon/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules/.pnpm/nodemon@2.0.14/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/node_modules:/mnt/c/Users/Gautam/Documents/Programming/node_modules:/mnt/c/Users/Gautam/Documents/node_modules:/mnt/c/Users/Gautam/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules/.pnpm/nodemon@2.0.14/node_modules/nodemon/bin/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules/.pnpm/nodemon@2.0.14/node_modules/nodemon/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules/.pnpm/nodemon@2.0.14/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules/.pnpm/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/backend/node_modules:/mnt/c/Users/Gautam/Documents/Programming/Movie Hub/node_modules:/mnt/c/Users/Gautam/Documents/Programming/node_modules:/mnt/c/Users/Gautam/Documents/node_modules:/mnt/c/Users/Gautam/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/nodemon.js" "$@"
else
  exec node  "$basedir/../../bin/nodemon.js" "$@"
fi
